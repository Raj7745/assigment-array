
#include<stdio.h>
#include<conio.h>
#define size 5
int main()
{
    int arr[size]={0};
    int i=0;
    int  ocount=0;
   
   printf(" enter array %d elements\n",size);
   
   for(i=0 ; i < size ; i++)
   {
      printf(" Array %d element \n",i+1);
      scanf("%d",&arr[i]);
   }
   printf("display elements\n");
   getch();
   for(i=0 ; i<size ; i++)
   {
    if(arr[i] == 0)
    {
      continue;
    }
    
    if(arr[i] % 2== 1 )
    {
      printf("%d is odd\n",arr[i]);
      ocount++;
    }
    
   }
   printf("odd count of an array is %d \n",ocount);
   
   return 0;
}
